{
	"application": {
		"port": "$APP_PORT",
		"stackErrorVisible": false
	},
	"db": {
		"database": "$DB_NAME",
		"username": "$DB_USERNAME",
		"password": "$DB_PASSWORD",
		"host": "$DB_HOST",
		"charset": "utf8",
		"dialect": "$DB_DBMS_NAME",
		"port": "$DB_PORT",
		"define": {
			"underscored": false,
			"timestamps": true,
			"freezeTableName": false
		}
	},
	"integration": {
		"aws": {
			"congito": {},
			"s3": {},
			"sqs": {},
			"sns": {}
		},
		"redis": {
			"sessionManager": "$REDIS_SESSION_MANAGER"
		}
	}
}
